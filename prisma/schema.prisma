generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model user {
  id        Int         @id @default(autoincrement())
  email     String
  name      String?
  password  String?
  active    Boolean?
  namespace namespace[]
}

model namespace {
  id      Int     @id @default(autoincrement())
  name    String?
  user_id Int?
  user    user?   @relation(fields: [user_id], references: [id], onDelete: Cascade)
}
